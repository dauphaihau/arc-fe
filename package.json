{
  "name": "web-arc-app",
  "private": true,
  "type": "module",
  "author": {
    "name": "Hau Tran",
    "email": "hautran.job@outlook.com",
    "url": "https://github.com/dauphaihau"
  },
  "description": "Web app for arc app build with nuxt 3, typescripts, pinia, tailwind, nuxt/ui, zod",
  "scripts": {
    "generate": "nuxt generate",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "devtools:enable": "npx nuxi@latest devtools enable",
    "devtools:disable": "npx nuxi@latest devtools disable",
    "typecheck": "npx nuxi typecheck",
    "test": "vitest --run tests",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --fix .",
    "postinstall": "nuxt prepare && husky install",
    "prepare": "husky install",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "pnpm lint:fix",
      "pnpm test"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/image": "^1.6.0",
    "@nuxt/test-utils": "^3.9.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.0.0",
    "@samk-dev/nuxt-vcalendar": "^1.0.2",
    "@types/eslint__eslintrc": "^2.1.1",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^10.7.0",
    "@vueuse/nuxt": "^10.7.0",
    "eslint": "^9.5.0",
    "eslint-plugin-import-x": "^0.5.1",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "nuxt": "^3.12.2",
    "nuxt-security": "2.0.0-rc.7",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "vue": "^3.3.12",
    "vue-eslint-parser": "^9.3.2",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@nuxt/ui": "^2.17.0",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/vue-query": "^5.45.0",
    "dayjs": "^1.11.10",
    "http-status-codes": "^2.3.0",
    "pinia": "^2.1.7",
    "zod": "^3.22.4"
  },
  "web-types": "./web-types.json"
}
